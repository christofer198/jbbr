<h1>Your Matches</h1>
<% if session[:type] == 1 %>
  <% @user.user_matches.each do |match| %>
  <%= match.opening.title %>
  <br>
  <%= match.opening.description %>
  <br>
  <%= match.opening.salary %>
  <br>
  <%= match.opening.zip_code %>
  <br>
  <%= match.opening.company.name%>
  <% end %>
<% else %>
  <% @user.company_matches.each do |match| %>
    <%= match.opening.title %>
    <br>
    <%= match.applicant.name %>
  <% end %>
<% end %>
